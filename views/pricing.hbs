<!-- Pricing Page Styles -->
<style>
  /* Custom CSS to mimic the original design */ 
  .pricing-section { 
    padding: 24px 6%; 
    position: relative; 
    z-index: 1; 
    background-color: #000000; 
    color: white; 
  } 

  .pricing-section .background-shape,
  .background-wrap { 
    display: none;
  }
  .pricing-card {
    background-color: #000000;
    color: white;
    border-radius: 1.5rem;
    padding: 2rem;
    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.25);
    margin-bottom: 2rem;
  }

  .unlock-button-free {
    width: 380px;
    padding: 12px 48px;
  }
  
  .pricing-card .feature {
    display: flex;
    align-items: center;
    margin-top: 0.75rem;
  }
  .pricing-card.featured {
    background-color: #4526b0;
    color: white;
  }
  .pricing-section .container {
    background-color: #000000;
  }
  .text-center {
    background-color: #000000;
  }
  .pricing-section h2 {
    font-weight: 600;
  }
  .pricing-section p.subtitle {
    font-size: 2.5rem;
    font-weight: 600;
    text-align: center;
    margin-top: 2rem;
  }
  .pricing-section p.description {
    text-align: center;
    margin-top: 2rem;
  }
  .btn {
    display: none !important;
  }

  /* Animation styles */
  .feature {
    opacity: 0;
    transform: translateY(20px);
    transition: opacity 0.5s ease, transform 0.5s ease;
  }

  .feature.animate {
    opacity: 1;
    transform: translateY(0);
  }

  .pricing-container {
    width: 838px !important;
    margin: 20px auto !important;
  }

  #premium-plan-content, #free-plan-content {
    display: flex !important;
  }

  #free-plan-content {
    display: none !important;
  }

  #free-plan-content.active {
    display: flex !important;
  }

  #premium-plan-content.hidden {
    display: none !important;
  }

  @media (max-width: 768px) {
    #premium-plan-content {
      display: flex !important;
      flex-direction: column;
    }

    #free-plan-content {
      display: none;
      flex-direction: column !important;
    }

    #free-plan-content.active {
      display: flex !important;
    }

    #premium-plan-content.hidden {
      display: none !important;
    }

    .pricing-title{
      font-size: 30px !important;
    }

    .pricing-card {
      padding-inline: 10px !important;
    }

    .pricing-container {
      width: 100% !important; 
      padding-inline: 20px !important;
    }

    .image-carousel {
      width: 90% !important;
    }

    .carousel-image {
      height: 100% !important;
    }

    .price-dollar{
      left: -10px !important;
    }

    .unlock-button {
      width: 100% !important;
      padding: 12px 48px 12px 48px !important;
      margin-top: 0px !important;
    }

    .unlock-button-free {
      padding: 12px 20px 12px 20px !important;
      width: 100% !important;
      margin-top: 0px !important;
    }
  }

  /* Add carousel styles */
  .image-carousel {
    position: relative;
    width: 380px;
    height: 316px;
    overflow: hidden;
    border-radius: 20px;
    background-color: #000000;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .carousel-image {
    position: absolute;
    inset: 0;
    width: 100%;
    height: 110%;
    object-fit: cover;
    object-position: center;
    opacity: 0;
    left: 0;
    transition: opacity 0.5s ease-in-out;
    background-color: #000000;
  }

  .carousel-image.active {
    opacity: 1;
  }

  .flex-app {
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 20px;
    margin-bottom: 20px;
  }

  .app-btn {
    width: 45%;
    max-width: 220px;
    color: #fff;
    margin: 20px 10px;
    text-align: left;
    border-radius: 5px;
    text-decoration: none;
    font-family: "Lucida Grande", sans-serif;
    font-size: 15px;
    text-transform: uppercase;
    border: 1px #3996fb solid;
    padding: 10px 10px;
  }

  .app-btn.blu {
    background-color: #101010;
    transition: background-color 0.25s linear;
  }

  .app-btn.blu:hover {
    background-color: #454545;
  }

  .app-btn i {
    width: 20%;
    text-align: center;
    font-size: 36px;
    margin-right: 7px;
  }

  .app-btn .big-txt {
    font-size: 25px;
    text-transform: capitalize;
  }

  /* Media query for screens below 768 pixels */
  @media screen and (max-width: 768px) {
    .flex-app {
      flex-direction: column; /* Stack children vertically */
    }

    .app-btn {
      width: 90%; /* Adjust the button width to take more space */
      margin: 7px auto; /* Center the buttons horizontally */
    }
    
    .app-btn i {
      margin-bottom: 7px; /* Add space below the icon */
    }
    
    .app-btn span {
      margin-top: 7px; /* Add space above the text */
    }
  }

  /* Desktop Menu Styles */
  .dropdown-content {
    display: none !important;
    position: absolute;
    min-width: 130px;
    box-shadow: 0px 8px 16px rgba(0, 0, 0, 0.2);
    z-index: 1;
    border-radius: 10px;
    background-color: #1F253B;
  }

  .dropdown:hover .dropdown-content {
    display: block !important;
    border-radius: 10px;
  }

  .dropdown-content a {
    margin-block: 10px;
    color: #ffffff;
    padding: 7px 10px;
    text-decoration: none;
    display: block;
  }

  .dropdown-content a:hover {
    background-color: #121524;
  }

  .dropdown {
    position: relative;
  }

  .dropdown-content div {
    /* Empty rule */
  }
</style>

